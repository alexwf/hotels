@model List<SisEventos.Models.Usuario>
@{
    ViewData["Title"] = "Index";
}

<h2>Usuários</h2>

<table class="table">
    <thead>
        <tr>
            <td>Nome</td>
            <td>E-mail</td>
            <td>ID</td>
            
        </tr>
    </thead>

    <tbody>
        @foreach (Usuario usuario in Model)
        {
            <tr>
                <td>@usuario.Nome</td>
                <td>@usuario.Email</td>
                <td>@usuario.Id</td>
               
            </tr>
        }
    </tbody>
    
</table>
